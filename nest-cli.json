{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/q-micro-services/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/q-micro-services/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/q-micro-services",
  "projects": {
    "q-micro-services": {
      "type": "application",
      "root": "apps/q-micro-services",
      "entryFile": "main",
      "sourceRoot": "apps/q-micro-services/src",
      "compilerOptions": {
        "tsConfigPath": "apps/q-micro-services/tsconfig.app.json"
      }
    },
    "course": {
      "type": "application",
      "root": "apps/course",
      "entryFile": "main",
      "sourceRoot": "apps/course/src",
      "compilerOptions": {
        "tsConfigPath": "apps/course/tsconfig.app.json"
      }
    }
  }
}